інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  крок початок:
    %резн = накопичити ц32
    %ан = накопичити ц32
    %бн = накопичити ц32
    записати ц32 %аарг %ан
    записати ц32 %барг %бн
  крок додавання:
    %а1 = прочитати ц32 %ан
    %б1 = прочитати ц32 %бн
    %рез1 = додати ц32 %а1 %б1
    записати ц32 %рез1 %резн
    стрибнути вихід
  крок ігнор:
    %а2 = прочитати ц32 %ан
    %б2 = прочитати ц32 %бн
    %рез3 = відняти ц32 %а1 %б1
  крок вихід:
    %рез2 = прочитати ц32 %резн
    вернути %рез2
}
