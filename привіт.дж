інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  блок початок:
    %резн = накоп ц32
    %ан = накоп ц32
    %бн = накоп ц32
    запис ц32 %аарг %ан
    запис ц32 %барг %бн
  блок програма:
    %а1 = завнт %ан
    %б1 = завнт %бн
    %рез1 = додат ц32 %а1 %б1
    запис ц32 %рез1 %резн
    стриб вихід
  блок вихід:
    вернт %резн
}
