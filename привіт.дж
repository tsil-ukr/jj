інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  блок початок:
    %резн = нак ц32
    %ан = нак ц32
    %бн = нак ц32
    зап ц32 %аарг %ан
    зап ц32 %барг %бн
  блок програма:
    %а1 = зав %ан
    %б1 = зав %бн
    %рез1 = дод ц32 %а1 %б1
    зап ц32 %рез1 %резн
    стр вихід
  блок вихід:
    вер %резн
}

інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  блок початок:
    %резн = нкп ц32
    %ан = нкп ц32
    %бн = нкп ц32
    зпс ц32 %аарг %ан
    зпс ц32 %барг %бн
  блок програма:
    %а1 = звт %ан
    %б1 = звт %бн
    %рез1 = ддт ц32 %а1 %б1
    зпс ц32 %рез1 %резн
    стр вихід
  блок вихід:
    врн %резн
}

інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  блок початок:
    %резн = накоп ц32
    %ан = накоп ц32
    %бн = накоп ц32
    запис ц32 %аарг %ан
    запис ц32 %барг %бн
  блок програма:
    %а1 = завнт %ан
    %б1 = завнт %бн
    %рез1 = додат ц32 %а1 %б1
    запис ц32 %рез1 %резн
    стриб вихід
  блок вихід:
    вернт %резн
}

інструкція щось_зробити(%аарг: ц32, %барг: ц32) -> ц32 {
  блок початок:
    %резн = накопичити ц32
    %ан = накопичити ц32
    %бн = накопичити ц32
    записати ц32 %аарг %ан
    записати ц32 %барг %бн
  блок програма:
    %а1 = завантажити %ан
    %б1 = завантажити %бн
    %рез1 = додати ц32 %а1 %б1
    записати ц32 %рез1 %резн
    стрибнути вихід
  блок вихід:
    вернути %резн
}
